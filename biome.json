{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"correctness": {
				"useJsxKeyInIterable": "off"
			},
			"suspicious": {
				"noExplicitAny": "warn",
				"noConfusingVoidType": "off"
			},
			"complexity": {
				"useLiteralKeys": "off",
				"noBannedTypes": "off"
			},
			"a11y": {
				"noSvgWithoutTitle": "off",
				"useAltText": "warn"
			},
			"style": {
				"noCommaOperator": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": false
	}
}